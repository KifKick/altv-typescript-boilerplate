{
	"name": "altv-typescript-boilerplate",
	"version": "1.0.0",
	"description": "altv typescript boilerplate with TypeORM",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build": "npm run build:server && npm run build:client",
		"watch:server": "rollup --watch --config resources/boilerplate/source/server/rollup.config.ts",
		"build:server": "rollup --config resources/boilerplate/source/server/rollup.config.ts",
		"watch:client": "rollup --watch --config resources/boilerplate/source/client/rollup.config.ts",
		"build:client": "rollup --config resources/boilerplate/source/client/rollup.config.ts"
	},
	"author": "KifKick",
	"license": "UNLICENSED",
	"dependencies": {
		"fs-extra": "^9.0.0",
		"mysql": "^2.18.1",
		"typeorm-esm": "^0.2.15-dev10",
		"chalk": "^3.0.0",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@altv/native-types": "^1.0.3",
		"@altv/types": "^1.0.17",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"@types/fs-extra": "^8.1.0",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.9.3",
		"@typescript-eslint/eslint-plugin": "^2.25.0",
		"@typescript-eslint/eslint-plugin-tslint": "^2.25.0",
		"@typescript-eslint/parser": "^2.25.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-import": "^2.20.1",
		"lodash": "^4.17.15",
		"rollup": "^2.1.0",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-eslint": "^7.0.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-terser": "^5.3.0",
		"rollup-plugin-typescript2": "^0.26.0",
		"typescript": "^3.8.3"
	}
}
